{
  "name": "beamerstream_app",
  "version": "1.0.0",
  "description": "Beamerstream App",
  "main": "./build/index.js",
  "scripts": {
    "start": "electron build/",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --win --x64",
    "postinstall": "electron-builder install-app-deps",
    "preelectron-pack": "npm run-script compile",
    "compile": "tsc"
  },
  "author": "Appinda",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^12.12.42",
    "electron": "^9.0.0",
    "electron-builder": "^22.7.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@types/consola": "^2.2.5",
    "@types/electron": "^1.6.10",
    "@types/express": "^4.17.6",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jquery": "^3.5.1",
    "socket.io": "^2.3.0",
    "uuid": "^8.1.0"
  },
  "build": {
    "appId": "beamerstream.desktop",
    "asar": false,
    "mac": {
      "category": "public.app-category.graphics-design"
    },
    "win": {
      "target": "portable"
    },
    "directories": {
      "buildResources": "."
    },
    "files": [
      "assets/**/*",
      "public/**/*",
      "build/**/*",
      "package.json"
    ]
  }
}
