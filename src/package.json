{
  "name": "beamerstream",
  "version": "1.0.0",
  "description": "Beamerstream App",
  "author": "Appinda",
  "main": "src/main.js",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "cross-env nuxt build",
    "start": "tsc && electron .",
    "generate": "nuxt generate",
    "pack": "tsc && yarn generate && electron-builder --dir",
    "dist": "tsc && yarn generate && electron-builder --x64 --win zip --win nsis",
    "postinstall": "electron-builder install-app-deps",
    "clean": "rimraf wwwroot && rimraf dist && rimraf src"
  },
  "dependencies": {
    "@nuxtjs/apollo": "^4.0.1-rc.1",
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/dotenv": "^1.4.0",
    "@types/accepts": "^1.3.5",
    "@types/express": "^4.17.6",
    "@types/socket.io": "^2.1.8",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-server": "^2.15.0",
    "apollo-server-express": "1",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.1.3",
    "bootstrap-vue": "^2.0.0",
    "core-js": "3",
    "express-graphql": "^0.9.0",
    "graphql": "^15.3.0",
    "graphql-subscriptions": "^1.1.0",
    "graphql-tag": "^2.10.3",
    "graphql-tools": "^6.0.11",
    "jquery": "^3.5.1",
    "nuxt": "^2.0.0",
    "rimraf": "^3.0.2",
    "subscriptions-transport-ws": "^0.9.16",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "@babel/runtime-corejs3": "^7.10.3",
    "@nuxtjs/style-resources": "^1.0.0",
    "@types/node": "^12.0.0",
    "@types/yargs": "^15.0.5",
    "electron": "^9.0.2",
    "electron-builder": "^22.7.0",
    "node-sass": "^4.14.1",
    "sass-loader": "^8.0.2",
    "typescript": "^3.9.5"
  },
  "resolutions": {
    "core-js": "2.6.11"
  },
  "build": {
    "win": {},
    "asar": true,
    "files": [
      "src",
      "wwwroot",
      "**/node_modules/*/**",
      "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme}",
      "!**/node_modules/*/{test,__tests__,tests,powered-test,example,examples}",
      "!**/node_modules/*.d.ts",
      "!**/node_modules/.bin",
      "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
      "!.editorconfig",
      "!**/._*",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,.gitignore,.gitattributes}",
      "!**/{__pycache__,thumbs.db,.flowconfig,.idea,.vs,.nyc_output}",
      "!**/{appveyor.yml,.travis.yml,circle.yml}",
      "!**/{npm-debug.log,yarn.lock,.yarn-integrity,.yarn-metadata.json}"
    ]
  }
}
