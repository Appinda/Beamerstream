{
  "name": "beamerstream",
  "version": "1.0.0",
  "description": "Beamerstream App",
  "author": "Appinda",
  "main": "src/main.js",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "tsc && yarn generate && electron .",
    "generate": "nuxt generate",
    "pack": "tsc && yarn generate && electron-builder --dir",
    "dist": "tsc && yarn generate && electron-builder --x64 --win zip --win nsis",
    "postinstall": "electron-builder install-app-deps",
    "clean": "rimraf wwwroot && rimraf dist && rimraf src"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/dotenv": "^1.4.0",
    "@types/express": "^4.17.6",
    "@types/socket.io": "^2.1.8",
    "bootstrap": "^4.1.3",
    "bootstrap-vue": "^2.0.0",
    "nuxt": "^2.0.0",
    "rimraf": "^3.0.2",
    "socket.io": "^2.3.0",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "@types/node": "^12.0.0",
    "@types/yargs": "^15.0.5",
    "electron": "^9.0.2",
    "electron-builder": "^22.7.0",
    "node-sass": "^4.14.1",
    "sass-loader": "^8.0.2",
    "typescript": "^3.9.5"
  },
  "build": {
    "win": {},
    "asar": true,
    "files": [
      "src",
      "wwwroot",
      "**/node_modules/*/**",
      "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme}",
      "!**/node_modules/*/{test,__tests__,tests,powered-test,example,examples}",
      "!**/node_modules/*.d.ts",
      "!**/node_modules/.bin",
      "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
      "!.editorconfig",
      "!**/._*",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,.gitignore,.gitattributes}",
      "!**/{__pycache__,thumbs.db,.flowconfig,.idea,.vs,.nyc_output}",
      "!**/{appveyor.yml,.travis.yml,circle.yml}",
      "!**/{npm-debug.log,yarn.lock,.yarn-integrity,.yarn-metadata.json}"
    ]
  }
}
